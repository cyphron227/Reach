{
  "name": "reach",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild:capacitor": "node -e \"var fs=require('fs');['src/app/auth/callback/route.ts','middleware.ts'].forEach(f=>{try{fs.renameSync(f,f+'.bak')}catch(e){}})\"",
    "build:capacitor": "cross-env STATIC_EXPORT=true next build",
    "postbuild:capacitor": "node -e \"var fs=require('fs');['src/app/auth/callback/route.ts.bak','middleware.ts.bak'].forEach(f=>{try{fs.renameSync(f,f.replace('.bak',''))}catch(e){}})\"",
    "start": "next start",
    "lint": "next lint",
    "deploy": "git push && npm run build:capacitor && npx cap sync android"
  },
  "dependencies": {
    "@capacitor-community/contacts": "^7.1.0",
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.91.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "libphonenumber-js": "^1.12.36",
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
